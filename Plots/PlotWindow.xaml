<Window x:Class="Plots.PlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        xmlns:local="clr-namespace:Plots"
        xmlns:cc="clr-namespace:Plots"
        mc:Ignorable="d"
        x:Name="rollingplot"
        Title="Rolling Plot View" Width="320" Height="240">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Load" Command="{Binding LoadRollingRpmCommand}"></MenuItem>
        </Menu>
        <ContentControl Content="{Binding PlotPageViewModel}"  d:Height="200" d:Width="400"></ContentControl>
    </DockPanel>
</Window>
